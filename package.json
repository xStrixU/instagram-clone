		"react-hook-form": "7.51.4",
		"tailwind-merge": "2.3.0",
		"ts-pattern": "5.1.1",
		"use-debounce": "10.0.0",
		"vitest": "1.6.0",
		"zod": "3.23.8",
		"zod-form-data": "2.0.2"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "1.4.0",
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"@commitlint/types": "19.0.3",
